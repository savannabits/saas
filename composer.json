{
    "name": "savannabits/saas",
    "description": "SaaS starter for FilamentPHP projects",
    "keywords": [
        "savannabits",
        "laravel",
        "saas"
    ],
    "homepage": "https://github.com/savannabits/saas",
    "support": {
        "issues": "https://github.com/savannabits/saas/issues",
        "source": "https://github.com/savannabits/saas"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Maosa",
            "email": "maosa.sam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "amrshawky/laravel-currency": "^6.0",
        "beyondcode/laravel-websockets": "^1.14.0",
        "bezhansalleh/filament-shield": "^3.0",
        "coolsam/excel-import": "^1.0",
        "directorytree/ldaprecord-laravel": "^3.0",
        "filament/filament": "^3.0",
        "filament/spatie-laravel-media-library-plugin": "^3.0",
        "filament/spatie-laravel-settings-plugin": "^3.0",
        "filament/spatie-laravel-translatable-plugin": "^3.0-stable",
        "illuminate/contracts": "^10.0",
        "kalnoy/nestedset": "^6.0.2",
        "laravel-ready/readable-numbers": "^1.0.3",
        "pragmarx/countries": "^0.8.2",
        "psr/http-message": "^1.0",
        "psr/simple-cache": "^2.0",
        "pxlrbt/filament-excel": "^2.1",
        "spatie/laravel-package-tools": "^1.15.0",
        "spatie/laravel-permission": "^5.10.2",
        "spatie/laravel-settings": "^3.2.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Savannabits\\Saas\\": "src/",
            "Savannabits\\Saas\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Savannabits\\Saas\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Savannabits\\Saas\\SaasServiceProvider"
            ],
            "aliases": {
                "Saas": "Savannabits\\Saas\\Facades\\Saas"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
